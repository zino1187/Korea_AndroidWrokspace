<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.koreait.pageapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PageApp">
        <activity android:name=".intent.FormActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".intent.ReceiveActivity"></activity>
        <activity android:name=".fragment.PagerActivity">
        </activity>
        <activity android:name=".activity.MainActivity"></activity>
        <activity android:name=".activity.RedActivity" />
        <activity android:name=".activity.BlueActivity" />
        <activity android:name=".activity.YellowActivity" />
    </application>

</manifest>